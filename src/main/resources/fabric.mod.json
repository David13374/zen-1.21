{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "QOL Mod",
	"authors": ["Kiwi"],
	"contact": {
		"homepage": "https://meowing.xyz/",
		"sources": "https://github.com/StellariumMC/zen-1.21"
	},
	"license": "GPL-3.0-or-later",
	"icon": "assets/zen/icon.png",
	"environment": "*",
	"entrypoints": {
		"client": [
			{
				"value": "xyz.meowing.zen.Zen",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			{
				"value": "xyz.meowing.zen.compat.ModMenu",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mixins.zen.json",
		{
			"config": "meowing.zen.mixins.json",
			"environment": "*"
		}
	],
	"depends": {
      "minecraft": "~${minor_mc_version}",
      "fabric": "*",
      "fabricloader": ">=0.15.11",
      "fabric-language-kotlin": "*"
	},
	"suggests": {
		"modmenu": "*"
	}
}
